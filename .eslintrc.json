{
    "env": {
        "browser": true,
        "es6": true,
	"jasmine": true
    },
    "extends":[
        "eslint:recommended",
        "plugin:jasmine/recommended"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "jasmine"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
	"no-unused-vars": "warn"
    }
}
